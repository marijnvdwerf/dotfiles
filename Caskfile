# Install native apps
# Usage: `brew bundle Caskfile`

install caskroom/cask/brew-cask
tap caskroom/versions

# browser
cask install google-chrome 2> /dev/null

# development
cask install android-studio 2> /dev/null
cask install imageoptim 2> /dev/null
cask install sequel-pro 2> /dev/null
cask install textmate 2> /dev/null

# other
cask install dropbox 2> /dev/null
cask install transmission 2> /dev/null
cask install vlc 2> /dev/null

# quick look plugins (https://github.com/sindresorhus/quick-look-plugins)
cask install qlcolorcode qlstephen qlmarkdown quicklook-json qlprettypatch quicklook-csv betterzipql webp-quicklook suspicious-package 2> /dev/null
qlmanage -r